<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <view
        android:id="@+id/content_container"
        class="com.tglt.sagittarius.views.DecorLayout$ContentFrameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <view
            class="com.tglt.sagittarius.views.DecorLayout$BackScrimView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="?settingsBackground" />

        <com.android.launcher3.InsettableFrameLayout
            android:id="@android:id/content"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <view
            class="com.tglt.sagittarius.views.DecorLayout$FrontScrimView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="?settingsBackground" />
    </view>
</merge>
